generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id    String @id @default(cuid())
  name  String
  slug  String @unique
  order Int    @unique

  @@map("categories")
}
